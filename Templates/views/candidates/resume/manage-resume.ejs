                    }
                    const skillRegex = /^[A-Za-z\s]+$/;

                    skillForm.addEventListener('submit', async (e) => {

                        const skillTitle = skillInput.value.trim();
                        if (skillTitle === "") {
                            e.preventDefault()
                            alert('plzz add skill')
                            return;
                        }

                        if (!skillRegex.test(skillTitle)) {
                            alert("skill name should only contain letters.");
                            e.preventDefault();
                            return;
                        }
                        skillBtn.textContent = 'Saving...';
                        skillBtn.disabled = true;
                        try {
                            if (response.ok) {
                                window.location.href = '/candidates/manage-resume?notification=201';
