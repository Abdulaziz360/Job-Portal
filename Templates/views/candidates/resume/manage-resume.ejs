                    }

                    const inputRegex = /^[A-Za-z]+$/;
                    empForm.addEventListener('submit', async (e) => {
                        const _designation = empinput_de.value.trim();
                        const _organization = empinput_org.value.trim();
                        const _jobDetail = empinput_detail.value.trim();

                        if (!inputRegex.test(_designation) || !inputRegex.test(_organization) || !inputRegex.test(_jobDetail)) {
                            alert("this field only contains letters");
                            e.preventDefault();
                            return;
                        }
                        if (_designation === "") {
                            alert("Please fill out the designation");
                            e.preventDefault();
                            return;
                        }

                        if (_organization === "") {
                            alert("Please fill out organization field.");
                            e.preventDefault();
                            return;
                        }
                        const start_dt = empinput_st_dat.value.trim();
                        const end_dt = empinput_en_dat.value.trim();
                        if (start_dt === "") {
                            alert("Please fill out starting date field.");
                            e.preventDefault();
                            return;
                        }
                        if (end_dt === "") {
                            alert("Please fill out ending date field.");
                            e.preventDefault();
                            return;
                        }
                        if (_jobDetail === "") {
                            alert("Please fill out detail field.");
                            e.preventDefault();
                            return;
                        }
                        const currentYear = new Date().getFullYear();
                        const startYear = new Date(start_dt).getFullYear();
                        const endYear = new Date(end_dt).getFullYear();

                        const currentMonth = new Date().getMonth()
                        const startMonth = new Date(start_dt).getMonth()
                        const endMonth = new Date(end_dt).getMonth()

                        const currentDay = new Date().getDay()
                        const startDay = new Date(start_dt).getDay()
                        const endDay = new Date(end_dt).getDay()

                        if (startYear > endYear) {
                            alert("Starting year cannot be greater than ending year.");
                            e.preventDefault();
                            return;
                        }

                        if (startYear > currentYear || endYear > currentYear) {
                            alert("Please select a year not greater than the current year.");
                            e.preventDefault();
                            return;
                        }
                        if (startYear == currentYear & endYear == currentYear || startYear == endYear) {
                            if (startMonth > endMonth) {
                                alert("date selection invalid");
                                e.preventDefault();
                                return;
                            }
                            if (startMonth==endMonth) {
                                if (startDay > endDay) {
                                    alert("date selection invalid");
                                    e.preventDefault();
                                    return;
                                }
                            }
                        }
                        if (endYear == currentYear || endYear == startYear) {
                            if (endMonth > currentMonth) {
                                alert("date selection invalid");
                                e.preventDefault();
                                return;
                            }
                            if (endMonth===currentMonth||endMonth===startMonth) {
                                if (endDay > currentDay) {
                                    alert("date selection invalidww");
                                    e.preventDefault();
                                    return;
                                }

                            }
                        }
                        else {
                            emBtn.textContent = 'Saving...';
                            emBtn.disabled = true;
                        }
                        try {


                            if (response.ok) {
                                window.location.href = '/candidates/manage-resume?notification=added';
                            } else {
                                throw new Error('An error occurred during form submission.');
                            }
                        } catch (error) {
                            showErrorNotification('There was an error uploading your resume.');
                        } finally {
                            emBtn.textContent = 'Save';
                            emBtn.disabled = false;
                        }
                    });
                });

