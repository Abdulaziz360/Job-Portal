                // //employet exp  store request

                document.addEventListener('DOMContentLoaded', () => {
                    const empForm = document.getElementById('emp_store_form');
                    const empinput_de = document.querySelector('[name="designation"]');
                    const empinput_org = document.querySelector('[name="organization"]');
                    const empinput_st_dat = document.querySelector('[name="working_start_date"]');
