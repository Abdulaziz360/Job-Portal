            <script>


                function showSuccessNotification(message) {
                    const notification = document.createElement('div');
                    notification.className = 'notification success';
                    notification.innerText = message;
                    document.body.appendChild(notification);

                    setTimeout(() => {
                        notification.style.animation = 'slideOut 0.5s forwards';
                        setTimeout(() => {
                            notification.remove();
                        }, 500);
                    }, 3000); // Show for 3 seconds
                }
                function showErrorNotification(message) {
                    const notification = document.createElement('div');
                    notification.className = 'notification error';
                    notification.innerText = message;
                    document.body.appendChild(notification);

                    setTimeout(() => {
                        notification.style.animation = 'slideOut 0.5s forwards';
                        setTimeout(() => {
                            notification.remove();
                        }, 500);
                    }, 8000); // Show for 3 seconds
                }

                document.addEventListener('DOMContentLoaded', () => {
                    const resumeForm = document.getElementById('upload_resume');
                    const resumeTitleInput = document.querySelector('[name="resume_title"]');
                    const resumeInput = document.querySelector('[name="resume"]');
                    const resumeBtn = document.getElementById('resumeBtn');


                    const notificationType = '<%=notificationType%>'
                    if (notificationType === 'success') {
                        // Display the success notification
                        showSuccessNotification('Your resume has been uploaded successfully');
                        history.replaceState({}, document.title, '/candidates/manage-resume');
                    }

                    const allowedFormats = ['txt', 'pdf'];
                    const titleRegex = /^[A-Za-z]+$/;

                    resumeForm.addEventListener('submit', async (e) => {
                        const fileTitle = resumeTitleInput.value.trim();
                        const fileName = resumeInput.value.trim();
                        const fileExtension = fileName.split('.').pop().toLowerCase();

                        if (!titleRegex.test(fileTitle)) {
                            alert("Resume title should only contain letters.");
                            e.preventDefault();
                            return;
                        }
                        if (fileTitle === "") {
                            alert("Please fill out the resume title.");
                            e.preventDefault();
                            return;
                        }

                        if (fileName === "") {
                            alert("Please choose a resume file.");
                            e.preventDefault();
                            return;
                        }

                        if (!allowedFormats.includes(fileExtension)) {
                            alert("Please upload a TXT or PDF file.");
                            e.preventDefault();
                            return;
                        }
                        else {
                            resumeBtn.textContent = 'Saving...';
                            resumeBtn.disabled = true;
                        }
                        try {
                            const formData = new FormData(resumeForm);
                            const response = await fetch(resumeForm.action, {
                                method: 'POST',
                                body: formData
                            });

                            if (response.ok) {
                                window.location.href = '/candidates/manage-resume?notification=success';
                            } else {
                                throw new Error('An error occurred during form submission.');
                            }
                        } catch (error) {
                            showErrorNotification('There was an error uploading your resume.');
                        } finally {
                            resumeBtn.textContent = 'Save';
                            resumeBtn.disabled = false;
                        }
                    });
                });
