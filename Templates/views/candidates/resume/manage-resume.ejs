                        }

                        if (institute_name === "") {
                            alert("Please fill out institute field.");
                            e.preventDefault();
                            return;
                        }
                        const integerRegex = /^[0-9]+$/;
                        if (!integerRegex.test(pass_year)) {
                            alert("This field only contains integers (0-9).");
                            e.preventDefault();
                            return;
                        }

                        if (!inputRegex.test(_degree) || !inputRegex.test(institute_name)) {
                            alert("this field only contains letters");
                            e.preventDefault();
                            return;
                        }
                        else {
                            eduBtn.textContent = 'Saving...';
                            eduBtn.disabled = true;
                        }
                        try {


                            if (response.ok) {
                                window.location.href = '/candidates/manage-resume?notification=added-edu';
                            } else {
                                throw new Error('An error occurred during form submission.');
                            }
                        } catch (error) {
                            showErrorNotification('There was an error uploading your resume.');
                        } finally {
                            eduBtn.textContent = 'Save';
                            eduBtn.disabled = false;
                        }
                    });
                });

                //education exp  store request

            </script>
